@{
    ViewData["Title"] = "Karyawan Cuti";
    ViewData["type"] = "Data";
    ViewData["Aplikasi"] = "Leave Application";
    Layout = "_LayoutMazer";
}


<div class="page-heading">
    <div class="row">
        <div class="col-12 col-md-6 order-md-1 order-last">
            <h3>Karyawan Cuti</h3>
            <p class="text-subtitle text-muted">Navbar will appear in top of the page.</p>
        </div>
        <div class="col-12 col-md-6 order-md-2 order-first">
            <nav aria-label="breadcrumb" class="breadcrumb-header float-start float-lg-end">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="index.html">Leave Application</a></li>
                    <li class="breadcrumb-item active" aria-current="page">
                        Karyawan Cuti
                    </li>
                </ol>
            </nav>
        </div>
    </div>
</div>
<div class="page-content">
    <section class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h4> <span class="text-primary"><i class="bi bi-list-nested"></i></span> Data Karyawan Cuti</h4>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="table" class="table table-hover">
                            <thead>
                                <tr>
                                    <th>No</th>
                                    <th>Nama Karyawan</th>
                                    <th>Pengajuan Cuti</th>
                                    <th>Masa Cuti</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
    </section>
</div>
@section Scripts{

    <script src="~/template-mazer/js-setting/Employee/spesial.js"></script>
}